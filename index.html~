<script type="text/javascript">
function toggle_body(){
    $("#topLog").toggle();
    $("#test").toggle();
    $("#login").toggle();

};

function toggleContent() {
  var contentId1 = document.getElementById("topLog");
  contentId1.style.display == "block" ? contentId1.style.display = "none":contentId1.style.display = "block"; 
  var contentId2 = document.getElementById("test");
  contentId2.style.display == "block" ? contentId2.style.display = "none":contentId2.style.display = "block"; 
  var contentId3 = document.getElementById("login");
  contentId3.style.display == "block" ? contentId3.style.display = "none":contentId3.style.display = "block"; 
}
</script>

